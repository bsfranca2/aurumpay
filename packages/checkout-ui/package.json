{
  "name": "@aurumpay/checkout-ui",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "style": "./dist/runtime/index.css",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "imports": {
    "#build/ui/*": "./.nuxt/ui/*.ts",
    "#build/ui.css": "./.nuxt/ui.css"
  },
  "style": "./dist/runtime/index.css",
  "files": [
    ".nuxt/ui",
    ".nuxt/ui.css",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "dev": "nuxi dev",
    "prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@aurumpay/lib": "workspace:*",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/schema": "^3.16.2",
    "@nuxtjs/i18n": "^9.5.4",
    "@tailwindcss/vite": "^4.1.5",
    "@vee-validate/valibot": "^4.15.0",
    "defu": "^6.1.4",
    "lucide-vue-next": "^0.508.0",
    "reka-ui": "^2.2.0",
    "result": "workspace:*",
    "tailwindcss": "^4.1.5",
    "valibot": "^1.0.0",
    "vee-validate": "^4.15.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.1",
    "nuxt": "^3.16.2",
    "vue-tsc": "^2.2.10"
  }
}
